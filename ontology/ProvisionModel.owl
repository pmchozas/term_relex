<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:prv="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
  <owl:Class rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#Establishment">
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#Creation"/>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="en">class of provisions establishing new institutional bodies</rdfs:comment>
    <rdfs:label xml:lang="en">establishment</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#RuleOnAction">
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#Regulative"/>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="en">class of regulative provisions which discipline actions</rdfs:comment>
    <rdfs:label xml:lang="en">rule on action</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#Attribution">
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#Constitutive"/>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="en">class of provisions attributing roles and powers</rdfs:comment>
    <rdfs:label xml:lang="en">attribution</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#ImplicitRight">
    <owl:disjointWith>
      <owl:Class rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#ExplicitRight"/>
    </owl:disjointWith>
    <owl:equivalentClass>
      <owl:Class rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#ExplicitDuty"/>
    </owl:equivalentClass>
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#Right"/>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="en">class of the Rights not expressed in the text, in correlative correspondence with Duties</rdfs:comment>
    <rdfs:label xml:lang="en">implicit right</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#ImplicitLiability">
    <owl:disjointWith>
      <owl:Class rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#ExplicitLiability"/>
    </owl:disjointWith>
    <owl:equivalentClass>
      <owl:Class rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#ExplicitPower"/>
    </owl:equivalentClass>
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#Liability"/>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="en">class of the Liabilities not expressed in the text, in correlative correspondence with Powers</rdfs:comment>
    <rdfs:label xml:lang="en">implicit liability</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#Definition">
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#Constitutive"/>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="en">class of provisions defining and describing entities of a regulated reality</rdfs:comment>
    <rdfs:label xml:lang="en">definition</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#Violation">
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#Remedy"/>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="en">class of provisions that describe the violation of a prohibition, typically linked to an ad-hoc penalty (or sanction)</rdfs:comment>
    <rdfs:label xml:lang="en">violation</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#TermDefinition">
    <rdfs:subClassOf rdf:resource="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#Definition"/>
    <rdfs:comment xml:lang="en">class of provisions defining and describing terms introducing entities of a regulated reality</rdfs:comment>
    <rdfs:label xml:lang="en">term definition</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#Power">
    <owl:unionOf rdf:parseType="Collection">
      <owl:Class rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#ImplicitPower"/>
      <owl:Class rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#ExplicitPower"/>
    </owl:unionOf>
    <rdfs:subClassOf rdf:resource="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#Attribution"/>
    <rdfs:comment xml:lang="en">class of provisions establishing the possibility of a subject to produce specific juridical effects, namely to establish, modify or cancel a juridical relationship</rdfs:comment>
    <rdfs:label xml:lang="en">power</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#ExtensionAmendment">
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#RuleOnRule"/>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="en">class of amendment provisions which extend or reduce
the cases on which the norm operates</rdfs:comment>
    <rdfs:label xml:lang="en">extension amendment</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#Duty">
    <owl:unionOf rdf:parseType="Collection">
      <owl:Class rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#ImplicitDuty"/>
      <owl:Class rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#ExplicitDuty"/>
    </owl:unionOf>
    <rdfs:subClassOf rdf:resource="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#RuleOnAction"/>
    <rdfs:comment xml:lang="en">class of provisions indicating the behaviour imposed by a norm</rdfs:comment>
    <rdfs:label xml:lang="en">duty</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#Prohibition">
    <rdfs:subClassOf rdf:resource="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#RuleOnAction"/>
    <rdfs:comment xml:lang="en">class of provisions expressing a duty in negative form</rdfs:comment>
    <rdfs:label xml:lang="en">prohibition</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#Constitutive">
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#Rule"/>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="en">class of provisions which introduce or assign a juridical profiles
to entities of a regulated reality</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#Organization">
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#Creation"/>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="en">class of provisions which discipline the organization of new institutional bodies</rdfs:comment>
    <rdfs:label xml:lang="en">organization</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#Liability">
    <owl:unionOf rdf:parseType="Collection">
      <owl:Class rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#ImplicitLiability"/>
      <owl:Class rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#ExplicitLiability"/>
    </owl:unionOf>
    <rdfs:subClassOf rdf:resource="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#Attribution"/>
    <rdfs:comment xml:lang="en">class of provisions establishing a reduction or extension of the rights of a subject</rdfs:comment>
    <rdfs:label xml:lang="en">liability</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#ExplicitLiability">
    <rdfs:subClassOf rdf:resource="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#Liability"/>
    <rdfs:comment xml:lang="en">class of the liabilities explicitly expressed in a legislative text</rdfs:comment>
    <rdfs:label xml:lang="en">explicit liability</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#TemporalAmendment">
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#RuleOnRule"/>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="en">class of amendment provisions which modify the times of a norm
(come-into-force and efficacy times);</rdfs:comment>
    <rdfs:label xml:lang="en">temporal amendment</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#Procedure">
    <rdfs:subClassOf rdf:resource="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#Definition"/>
    <rdfs:comment xml:lang="en">class of provisions defining and describing procedures, namely the modality of compliance of a provision regulative of actions</rdfs:comment>
    <rdfs:label xml:lang="en">procedure</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#Permission">
    <rdfs:subClassOf rdf:resource="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#RuleOnAction"/>
    <rdfs:comment xml:lang="en">class of provisions indicating behaviours allowed by competent authorities, even derogating from other provisions</rdfs:comment>
    <rdfs:label xml:lang="en">permission</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#Redress">
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#Remedy"/>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="en">class of provisions that describe the effects of non-compliance with constitutive norms; the result is not a sanction or a punishment, for it is not a breach or violation of any obligation, nor an offence, but nullity</rdfs:comment>
    <rdfs:label xml:lang="en">redress</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#ExplicitDuty">
    <rdfs:subClassOf rdf:resource="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#Duty"/>
    <rdfs:label xml:lang="en">explicit right</rdfs:label>
    <rdfs:comment xml:lang="en">class of the Duties explicitly expressed in a legislative text</rdfs:comment>
    <rdfs:label xml:lang="en">explicit duty</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#ImplicitDuty">
    <owl:disjointWith rdf:resource="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#ExplicitDuty"/>
    <owl:equivalentClass>
      <owl:Class rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#ExplicitRight"/>
    </owl:equivalentClass>
    <rdfs:subClassOf rdf:resource="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#Duty"/>
    <rdfs:comment xml:lang="en">class of the Duties not expressed in the text, in correlative correspondence with Rights</rdfs:comment>
    <rdfs:label xml:lang="en">implicit duty</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#Regulative">
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#Rule"/>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="en">class of provisions which discipline actions
or the substantial and procedural defaults (remedies)</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#ImplicitPower">
    <owl:disjointWith>
      <owl:Class rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#ExplicitPower"/>
    </owl:disjointWith>
    <owl:equivalentClass rdf:resource="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#ExplicitLiability"/>
    <rdfs:subClassOf rdf:resource="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#Power"/>
    <rdfs:comment xml:lang="en">class of the powers not expressed in the text, in correlative correspondence with liabilities</rdfs:comment>
    <rdfs:label xml:lang="en">implicit power</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#ContentAmendment">
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#RuleOnRule"/>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="en">class of amendment provisions which modify literally the content
of a norm, or their meaning without literal changes</rdfs:comment>
    <rdfs:label xml:lang="en">content amendment</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#ExplicitRight">
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#Right"/>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="en">class of the Rights explicitly expressed in a legislative text</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#Provision">
    <rdfs:comment xml:lang="en">Class including all the provisions types (as Term Definition, Procedure, Duty, Right, Power, as well as more technical ones as Insertion, Abrogation, Substitution, etc.) and related attributes (for example the Bearer of a Right, or the Definiendum of a Term Definition), reflecting the lawmaker directions. In the Provision Model, provision types are organised into two main groups: Rules (introducing entities or expressing deontic concepts) and Rules on Rules (different kinds of amendments). The values of provision attributes can be expressed by lexical units, or by concepts derived from thesauri/ontologies, able to provide additional information on the entities of the regulated domain. For each attribute both rdfs:domain and rdfs:range can be specified: rdfs:domain specifies the type of individuals a provision attribute applies to (e.g. the individuals of the class ExplicitDuty for a provision attribute hasExplicitDutyBearer); rdfs:range specifies the type of values of this provision attribute. Since legislative texts can deal with any aspects of the reality, the values of provision attributes which are modeled by OWL object properties may belong to any class of objects (individual of owl:Class). Note that this cannot be formally defined in the ontology in order to guarantee that the ontology is fully-compliant to the OWL 2 DL profile.</rdfs:comment>
    <rdfs:label xml:lang="en">provision</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#Rule">
    <rdfs:subClassOf rdf:resource="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#Provision"/>
    <rdfs:comment xml:lang="en">class of provisions introducing entities or expressing deontic concepts</rdfs:comment>
    <rdfs:label xml:lang="en">rule</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#ExplicitPower">
    <rdfs:subClassOf rdf:resource="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#Power"/>
    <rdfs:comment xml:lang="en">class of the powers explicitly expressed in a legislative text</rdfs:comment>
    <rdfs:label xml:lang="en">explicit power</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#RuleOnRule">
    <rdfs:subClassOf rdf:resource="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#Provision"/>
    <rdfs:comment xml:lang="en">class of provisions introducing different kinds of amendments (content, temporal and extension amendments)</rdfs:comment>
    <rdfs:label xml:lang="en">rule on rule</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#Remedy">
    <rdfs:subClassOf rdf:resource="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#Regulative"/>
    <rdfs:comment xml:lang="en">class of regulative provisions which discipline substantial and procedural defaults</rdfs:comment>
    <rdfs:label xml:lang="en">remedy</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#Status">
    <rdfs:subClassOf rdf:resource="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#Attribution"/>
    <rdfs:comment xml:lang="en">class of provisions assigning powers and duties to an addressee with specific requirements,classifying him in a particular category of people or in more restricted community</rdfs:comment>
    <rdfs:label xml:lang="en">status</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#Creation">
    <rdfs:subClassOf rdf:resource="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#Constitutive"/>
    <rdfs:comment xml:lang="en">class of provisions introducing entities, in the sense that they create previously not-existent entities, of a regulated reality</rdfs:comment>
    <rdfs:label xml:lang="en">creation</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#Right">
    <owl:unionOf rdf:parseType="Collection">
      <owl:Class rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#ImplicitRight"/>
      <owl:Class rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#ExplicitRight"/>
    </owl:unionOf>
    <rdfs:subClassOf rdf:resource="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#RuleOnAction"/>
    <rdfs:comment xml:lang="en">class of provisions which give the power to act to protect or satisfy a specific interest recognized and protected by the legal system</rdfs:comment>
    <rdfs:label xml:lang="en">right</rdfs:label>
  </owl:Class>
  <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#hasProcedureObject">
    <rdfs:domain rdf:resource="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#Procedure"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#hasDefiniens">
    <rdfs:domain rdf:resource="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#TermDefinition"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#hasProcedureCounterpart">
    <rdfs:domain rdf:resource="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#Procedure"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#hasEstablishmentAddressee">
    <rdfs:domain rdf:resource="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#Establishment"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#hasExplicitLiabilityCounterpart">
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#hasLiabilityCounterpart"/>
    </rdfs:subPropertyOf>
    <rdfs:domain rdf:resource="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#ExplicitLiability"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#hasOrganizationAddressee">
    <rdfs:domain rdf:resource="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#Organization"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#hasPenalty">
    <rdfs:domain rdf:resource="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#Violation"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#hasDutyAction">
    <rdfs:domain rdf:resource="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#Duty"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#hasRightObject">
    <rdfs:domain rdf:resource="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#Right"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#hasExplicitPowerCounterpart">
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#hasPowerCounterpart"/>
    </rdfs:subPropertyOf>
    <rdfs:domain rdf:resource="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#ExplicitPower"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#hasPermissionBearer">
    <rdfs:domain rdf:resource="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#Permission"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#hasImplicitPowerBearer">
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#hasPowerBearer"/>
    </rdfs:subPropertyOf>
    <owl:equivalentProperty rdf:resource="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#hasExplicitLiabilityCounterpart"/>
    <rdfs:domain rdf:resource="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#ImplicitPower"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#hasProhibitionObject">
    <rdfs:domain rdf:resource="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#Prohibition"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#hasDutyBearer">
    <rdfs:domain rdf:resource="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#Duty"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#hasPermissionObject">
    <rdfs:domain rdf:resource="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#Permission"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#hasLiabilityBearer">
    <rdfs:domain rdf:resource="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#Liability"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#hasExplicitRightBearer">
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#hasRightBearer"/>
    </rdfs:subPropertyOf>
    <rdfs:domain rdf:resource="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#ExplicitRight"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#hasPermissionAction">
    <rdfs:domain rdf:resource="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#Permission"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ontologies/def/ProvisionModel#hasEffect">
